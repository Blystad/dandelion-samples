<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
   
   <definition name="export.links.*" extends="layout">
      <put-attribute name="content" value="/WEB-INF/views/export/links/{1}.jsp" />
   </definition>

   <definition name="export.options.*" extends="layout">
      <put-attribute name="content" value="/WEB-INF/views/export/options/{1}.jsp" />
   </definition>
   
   <definition name="export.dom.*" extends="layout">
      <put-attribute name="content" value="/WEB-INF/views/export/dom/{1}.jsp" />
   </definition>
   
   <definition name="export.ajax.*" extends="layout">
      <put-attribute name="content" value="/WEB-INF/views/export/ajax/{1}.jsp" />
   </definition>
   
   <definition name="export.*" extends="layout">
      <put-attribute name="content" value="/WEB-INF/views/export/{1}.jsp" />
   </definition>
   
</tiles-definitions>